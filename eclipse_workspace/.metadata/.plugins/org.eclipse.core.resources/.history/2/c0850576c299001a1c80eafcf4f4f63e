package core.basic;

public class DecimalToRoman {
/**
 * This program converts Decimal number to Roman
 * 
 * 50 = L
 * 100 = C
 * 500 = D
 * 1000 = M
 * 
 * 40 = XL
 * 90 = XC
 * 
 * 400 = CD
 * 900 = CM
 * 
 * 1 = I [w]
 * 4 = IV [if]
 * 5 = V [if]
 * 9 = IX [if]
 * 10 = X [w]
 * 
 * 40 = XL [if]
 * 50 = L [if]
 * 90 = XC [if]
 * 100 = C [w]
 * 
 * 400 = CD [if]
 * 500 = D [if]
 * 900 = CM [if]
 * 1000 = M [w]
 * 
 */
	
	int n;
	public void set(int n)
	{
		this.n = n;
	}
	

}
